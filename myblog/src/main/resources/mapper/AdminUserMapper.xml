<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chuanandhe.firstblog.myblog.dao.TestDao">
    <select id="findUser"
            resultType="com.chuanandhe.firstblog.myblog.entity.AdminUser">
        select * from tb_admin_user
    </select>
    <insert id="insertUser"
            parameterType="user" >
        insert into sysuser (name,
                            age,
                            sex) value (
                #{name},
                #{age},
                #{sex} )
    </insert>
</mapper>